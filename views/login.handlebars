
<form class="form-horizontal" id="form-login">
  
  <div class="form-group">
    <label for="email-login" class="col-sm-2 control-label text-warning">Email:</label>
    <div class="col-sm-4">
      <input type="email" class="form-control" id="email-login" placeholder="" />
    </div>
  </div>
  
  <div class="form-group">
    <label for="password-login" class="col-sm-2 control-label text-warning">Password:</label>
    <div class="col-sm-4">
      <input type="password" class="form-control" id="password-login" placeholder="" />
    </div>
  </div>
  
  <div>
    <button type="submit" class="btn btn-warning" id="btn-login">Login</button>
  </div>

</form>
  
 
<script src="/javascript/login.js"></script>
  







